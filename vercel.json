{
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "frontend/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}